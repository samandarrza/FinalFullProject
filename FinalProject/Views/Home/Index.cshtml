                      <span class="review-text">
                        <span>100+</span> Reviews
                      </span>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-lg-3">
            <div class="slider-image">
              <img src="~/assets/images/@slider.Image" alt="">
            </div>
          </div>
        </div>
